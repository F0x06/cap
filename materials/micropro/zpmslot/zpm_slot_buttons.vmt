	"$normalmapalphaenvmapmask" "1"
	"$normalmapalphaphongmask"    "1"
	"$surfaceprop" "metal"
	"$selfillum" 1
 	"$phong" "1"
	"$phongexponent" "33"
	"$phongboost"	"1"
	"$phongfresnelranges"	"[1 8 4]"
	"$halflambert" "1"
